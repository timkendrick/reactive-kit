{
  "private": true,
  "name": "@reactive-kit/plugin-evaluate",
  "version": "0.0.1",
  "license": "MIT",
  "description": "ReactiveKit evaluate plugin",
  "author": "Tim Kendrick <timkendrick@gmail.com>",
  "type": "module",
  "main": "./lib.ts",
  "module": "./lib.ts",
  "scripts": {
    "build": "nx run build",
    "context": "nx run context",
    "docs": "nx run docs",
    "lint": "nx run lint",
    "test": "nx run test"
  },
  "pkg": {
    "type": "module",
    "main": "./lib/lib.cjs",
    "module": "/lib/lib.js",
    "types": "./lib.d.ts",
    "exports": {
      ".": {
        "import": "./lib/lib.js",
        "require": "./lib/lib.cjs"
      },
      "./effects": {
        "import": "./lib/effects.js",
        "require": "./lib/effects.cjs"
      },
      "./handlers": {
        "import": "./lib/handlers.js",
        "require": "./lib/handlers.cjs"
      },
      "./hooks": {
        "import": "./lib/hooks.js",
        "require": "./lib/hooks.cjs"
      },
      "./messages": {
        "import": "./lib/messages.js",
        "require": "./lib/messages.cjs"
      },
      "./middleware": {
        "import": "./lib/middleware.js",
        "require": "./lib/middleware.cjs"
      },
      "./mocks": {
        "import": "./lib/mocks.js",
        "require": "./lib/mocks.cjs"
      },
      "./tasks": {
        "import": "./lib/tasks.js",
        "require": "./lib/tasks.cjs"
      },
      "./types": {
        "import": "./lib/types.js",
        "require": "./lib/types.cjs"
      },
      "./utils": {
        "import": "./lib/utils.js",
        "require": "./lib/utils.cjs"
      }
    }
  },
  "devDependencies": {
    "@reactive-kit/actor": "workspace:*",
    "@reactive-kit/build-config": "workspace:*",
    "@reactive-kit/hash": "workspace:*",
    "@reactive-kit/interpreter": "workspace:*",
    "@reactive-kit/message": "workspace:*",
    "@reactive-kit/reactive-utils": "workspace:*",
    "@reactive-kit/types": "workspace:*",
    "@reactive-kit/utils": "workspace:*"
  },
  "peerDependencies": {
    "@microsoft/api-extractor": "^7",
    "eslint": "^8",
    "nx": "^21",
    "prettier": "^3",
    "typedoc": "^0.28",
    "typescript": "^5",
    "vite": "^6",
    "vitest": "^3"
  },
  "nx": {
    "includedScripts": []
  }
}
