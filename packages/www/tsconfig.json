{
  "$schema": "http://json.schemastore.org/tsconfig",
  "extends": [
    "@reactive-kit/build-config/templates/tsc/react.tsconfig.json",
    "astro/tsconfigs/strict"
  ],
  "include": [
    "public",
    "src",
    "lib.ts",
    "lib.test.ts",
    ".astro/types.d.ts",
    "*.config.ts",
    "package.json"
  ],
  "compilerOptions": {
    "jsx": "react-jsx",
    "jsxImportSource": "react",
    "baseUrl": ".",
    "paths": {
      "@/*": ["src/*"],
      "@examples/*": ["examples/dist/*"]
    }
  }
}
