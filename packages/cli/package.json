{
  "private": true,
  "name": "@reactive-kit/cli",
  "version": "0.0.1",
  "license": "MIT",
  "description": "ReactiveKit CLI runner",
  "author": "Tim Kendrick <timkendrick@gmail.com>",
  "type": "module",
  "main": "./index.ts",
  "scripts": {
    "build": "pnpm run build:lib && pnpm run build:loader && pnpm run build:pkg",
    "build:lib": "vite build",
    "build:docs": "typedoc",
    "build:pkg": "pnpm run --filter build-tools pkg $PWD $PWD/dist/package.json",
    "build:loader": "pnpm run build:loader:lib && pnpm run build:loader:pkg",
    "build:loader:lib": "vite build --config loader.vite.config.ts",
    "build:loader:pkg": "pnpm run --filter build-tools pkg $PWD/../loader $PWD/dist/node_modules/@reactive-kit/loader/package.json",
    "lint": "pnpm run '/^lint:.*/'",
    "lint:eslint": "eslint --ext js,jsx,cjs,mjs,ts,tsx,cts,mts .",
    "lint:typescript": "tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest watch"
  },
  "pkg": {
    "main": "./index.cjs",
    "bin": "./index.cjs"
  },
  "bundleDependencies": [
    "@reactive-kit/loader"
  ],
  "dependencies": {
    "@reactive-kit/loader": "workspace:*"
  },
  "devDependencies": {
    "@reactive-kit/build-config": "workspace:*",
    "@reactive-kit/handlers": "workspace:*",
    "@reactive-kit/hash": "workspace:*",
    "@reactive-kit/runtime": "workspace:*",
    "@reactive-kit/types": "workspace:*",
    "@reactive-kit/utils": "workspace:*"
  },
  "peerDependencies": {
    "eslint": "^8",
    "typedoc": "^0.25",
    "typescript": "^5",
    "vite": "^4",
    "vitest": "^0.34"
  }
}
